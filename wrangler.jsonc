{
  "name": "moltworker",
  "main": "src/index.ts",
  "compatibility_date": "2025-02-05",
  "compatibility_flags": ["nodejs_compat"],
  
  "durable_objects": {
    "bindings": [
      {
        "name": "MOLTBOT_STATE",
        "class_name": "MoltbotDurableObject"
      }
    ]
  },
  
  "migrations": [
    {
      "tag": "v1",
      "new_classes": ["MoltbotDurableObject"]
    }
  ],
  
  "r2_buckets": [
    {
      "binding": "MOLTBOT_FILES",
      "bucket_name": "moltbot-data"
    }
  ]
}
